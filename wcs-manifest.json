{
  "title": "NOI Maps",
  "description": "This web component shows where are located the offices, the laboratories, the companies and institution in  the NOI Techpark.",
  "descriptionAbstract": "NOI Techpark map app",
  "license": "AGPL-3.0-or-later",
  "repositoryUrl": "https://github.com/noi-techpark/webcomp-maps-noi.git",
  "shortName": "noi-maps",
  "copyrightHolders": [
    {
      "name": "NOI AG",
      "email": "info@noi.bz.it",
      "organization": "NOI S.p.A",
      "organizationUrl": "https://noi.bz.it"
    }
  ],
  "authors": [
    {
      "name": "Made In Cima",
      "email": "info@madeincima.it",
      "organization": "Made In Cima s.r.l.",
      "organizationUrl": "https://www.madeincima.it"
    }
  ],
  "image": "wcs-logo.png",
  "searchTags": [
    "map"
  ],
  "dist": {
    "basePath": "dist",
    "files": [
      "noi_maps_widget.min.js",
      "noi_maps_widget.min.js.LICENSE.txt"
    ],
    "scripts": [
      {
        "file": "noi_maps_widget.min.js",
        "parameter": ""
      }
    ]
  },
  "configuration": {
    "tagName": "map-view",
    "options": [
      {
        "key": "language",
        "type": "select",
        "label": "Language",
        "required": false,
        "options": {
          "values": [
            "en",
            "de",
            "it"
          ],
          "default": "en"
        }
      },
      {
        "key": "totem",
        "type": "select",
        "label": "Totem",
        "required": false,
        "options": {
          "default": "0",
          "values": [
            "0",
            "1"
          ]
        }
      },
      {
        "key": "hidezoom",
        "type": "select",
        "label": "Show or hide zoom buttons",
        "required": false,
        "options": {
          "default": "0",
          "values": [
            "0",
            "1"
          ]
        }
      },
      {
        "key": "style",
        "type": "text",
        "label": "Height of the component as css property",
        "required": false,
        "options": {
          "default": "height:500px"
        }
      }
    ]
  }
}
